<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

    <changeSet id="create_factor_table" author="liquibase-user" dbms="mysql">
        <createTable tableName="factor">
            <column name="factor_id" type="int">
                <constraints primaryKey="true" primaryKeyName="factor_pk"/>
            </column>
            <column name="factor_name" type="varchar(100)" >
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>


</databaseChangeLog>
